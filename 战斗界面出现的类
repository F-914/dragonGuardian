class LadderReward 
    --propertities
    isUnlock type: bool --该用户是否已经解锁
    isGet type: bool--该用户是否已经领取
end

class CurrencyReward extend LadderReward
    quantity type: number --货币数量
    name type: string --货币的名称，也就是类型，可能是钻石，也可以是金币
end
class TreasureBoxReward extend LadderReward
    treasureBox type:TreasureBox, 封装了一个宝箱类，这个宝箱类应该是通用的类
end
class UnknownCardReward extend LadderReward
    unknownCard type:UnknownCard, 封装了一个可以开出未知卡片的奖励， 这个同样应该是通用的类
end

class Ladder 
    map type: Map 存储了天梯钥匙数量和奖励的对应关系key = number value = LadderRewardd 
    keyQuantity type: number 用户当前的钥匙数量
end


class team
    positionOne type: Tower 这个类是表示塔的类，应该也是通用的类
    positionTwo type: Tower 同上
    positionThree type: Tower 同上
    positionFour type: Tower 同上
    positionFive type: Tower 同上
end
